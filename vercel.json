{
    "version": 2,
    "name": "zentrixui-demo",
    "builds": [
        {
            "src": "demo/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "../dist-demo"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "buildCommand": "npm run build:demo",
    "outputDirectory": "dist-demo",
    "installCommand": "npm install",
    "framework": "vite"
}